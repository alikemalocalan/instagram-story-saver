{
  "name": "instagram story saver",
  "description": "",
  "repository": "https://github.com/alikemalocalan/instagram-story-saver",
  "logo": "https://www.herokucdn.com/deploy/button.svg",
  "keywords": [
    "scala",
    "instagram"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-scala.git"
    }
  ],
  "env": {
    "USERNAME": {
      "description": "your instagram username"
    },
    "PASSWORD": {
      "description": "your instagram password"
    },
    "BUCKETNAME": {
      "description": "S3 bucket name",
      "value": "sample-bucket"
    },
    "SETTINGONS3": {
      "description": "do you wan to save your cookie to s3 for logout problem?",
      "value": "false"
    },
    "REGION": {
      "description": "S3 region",
      "value": "us-east-1"
    },
    "ENPOINT": {
      "description": "S3 service entpoint",
      "value": "s3.us-east-1.amazonaws.com"
    },
    "AWS_ACCESS_KEY_ID": {
      "description": "Aws acces key"
    },
    "AWS_SECRET_ACCESS_KEY": {
      "description": "Aws acces key secret"
    }
  }
}